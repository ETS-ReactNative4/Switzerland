{
    "compilerOptions": {
        "lib": ["dom", "es2016", "es2017.object"],
        "target": "esnext",
        "module": "esnext",
        "outDir": "dist",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "plugins": [
            {
                "transform": "@zoltu/typescript-transformer-append-js-extension/output/index.js",
                "after": true
            }
        ]
    },
    "exclude": ["node_modules"]
}
