{
  "name": "switzerland",
  "version": "1.1.0",
  "description": "Switzerland takes a functional approach to Web Components by applying middleware to your components. Supports Redux, mobx, attribute mutations, CSS variables, React-esque setState/state, etcâ€¦ out-of-the-box, along with Shadow DOM for style encapsulation and Custom Elements for interoperability.",
  "main": "src/switzerland.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "spec": "taskfile spec",
    "build": "taskfile build",
    "lint": "taskfile lint",
    "lint-js": "taskfile lint-js",
    "lint-fs": "taskfile lint-fs",
    "test": "taskfile test",
    "start": "taskfile start",
    "watch": "taskfile watch",
    "webpack-defaults": "webpack-defaults"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Switzerland.git"
  },
  "keywords": [
    "web components",
    "webcomponents",
    "components",
    "shadow dom",
    "custom elements"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Switzerland/issues"
  },
  "homepage": "https://github.com/Wildhoney/Switzerland#readme",
  "dependencies": {
    "css-url-parser": "~1.1.2",
    "picodom": "~0.1.0",
    "prop-types": "~15.5.10",
    "redux": "~3.7.2",
    "redux-thunk": "~2.2.0",
    "shortid": "~2.2.8",
    "sort-by": "~1.2.0",
    "transform-runtime": "0.0.0"
  },
  "devDependencies": {
    "ava": "~0.19.1",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.0.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-do-expressions": "~6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-plugin-transform-react-jsx": "~6.24.1",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-stage-2": "~6.24.1",
    "concurrently": "~3.5.1",
    "eslint": "~3.19.0",
    "eslint-config-xo-react": "~0.12.0",
    "eslint-plugin-react": "~7.1.0",
    "express": "~4.15.5",
    "fslint": "~1.1.0",
    "inline-css": "~2.2.3",
    "jsdom": "~11.2.0",
    "puppeteer": "git+https://github.com/GoogleChrome/puppeteer.git",
    "ramda": "~0.23.0",
    "taskfile": "~0.5.11",
    "uglifyjs-webpack-plugin": "~1.0.1",
    "webpack": "~3.0.0",
    "xo": "~0.18.2"
  },
  "ava": {
    "files": [
      "tests/*.test.js",
      "tests/**/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "space": 4,
    "esnext": true,
    "parser": "babel-eslint",
    "globals": [
      "window",
      "document",
      "customElements",
      "fetch",
      "navigator",
      "MutationObserver",
      "ShadowRoot",
      "HTMLElement",
      "HTMLScriptElement",
      "CustomEvent"
    ],
    "extends": "xo-react",
    "rules": {
      "curly": [
        "error",
        "multi-line"
      ],
      "padded-blocks": "off",
      "object-curly-spacing": [
        2,
        "always"
      ],
      "max-params": [
        1,
        5
      ],
      "unicorn/filename-case": "off",
      "unicorn/custom-error-definition": "off",
      "default-case": "off",
      "brace-style": "off",
      "no-constant-condition": "off",
      "no-unused-expressions": "off",
      "import/prefer-default-export": "off",
      "react/jsx-space-before-closing": "off",
      "react/jsx-tag-spacing": "off",
      "react/jsx-no-bind": "off",
      "no-void": "off",
      "react/forbid-component-props": "off",
      "import/extensions": [
        1,
        {
          "js": "never",
          "json": "always"
        }
      ]
    }
  },
  "pre-push": []
}
