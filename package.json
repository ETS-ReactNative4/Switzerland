{
  "name": "switzerland",
  "version": "0.2.11",
  "description": "Switzerland takes a functional approach to Web Components by applying middleware to your components. Supports Redux, mobx, attribute mutations, CSS variables, React-esque setState/state, etc... out-of-the-box, along with Shadow DOM for style encapsulation and Custom Elements for interoperability.",
  "main": "switzerland.js",
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=build webpack -p",
    "watch": "BABEL_ENV=build webpack --watch",
    "test": "npm run lint && npm run spec && npm run stats",
    "spec": "BABEL_ENV=build NODE_ENV=development ava",
    "stats": "./tests/stats/default.sh && ./bash/stats.sh && rimraf ./tests/stats/build || rimraf ./tests/stats/build",
    "karma": "npm run build && karma start karmafile.js --single-run",
    "lint": "xo src/*.js src/**/*.js tests/*.js tests/**/*.js !tests/karma/* !tests/stats/*",
    "cleanup": "rimraf node_modules",
    "start": "npm i --ignore-scripts && npm run build && BABEL_ENV=node babel example/server/default.js | node",
    "prepare": "./bash/prepare.sh",
    "postpublish": "./bash/postpublish.sh"
  },
  "files": [
    "switzerland.js",
    "middleware.js",
    "helpers",
    "middleware",
    "debug"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Switzerland.git"
  },
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Switzerland/issues"
  },
  "homepage": "https://github.com/Wildhoney/Switzerland#readme",
  "dependencies": {
    "axios": "^0.14.0",
    "css-url-parser": "^1.1.0",
    "escape-string-regexp": "^1.0.5",
    "html-to-vdom": "^0.7.0",
    "humps": "^2.0.0",
    "orderly-queue": "^0.1.5",
    "path-parse": "^1.0.5",
    "prop-types": "^0.2.0",
    "ramda": "^0.22.1",
    "sort-by": "^1.1.1",
    "virtual-dom": "^2.1.1"
  },
  "optionalDependencies": {
    "starwars": "^1.0.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.15.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.14.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "envify": "^1.2.1",
    "envify-loader": "^0.1.0",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "express": "^4.14.0",
    "fetch-mock": "^5.8.1",
    "fslint": "^1.1.0",
    "jasmine": "^2.5.2",
    "jsdom": "^9.8.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "moment": "^2.15.2",
    "opener": "^1.4.2",
    "rambda": "^0.3.4",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.6",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "virtual-dom": "^2.1.1",
    "webcomponents.js": "^0.7.23",
    "webpack": "^2.2.0",
    "whatwg-fetch": "^2.0.2",
    "xo": "^0.17.0"
  },
  "ava": {
    "files": [
      "tests/*.test.js",
      "tests/**/*.test.js",
      "!tests/karma/*.test.js",
      "!tests/stats/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "./tests/helpers/setup-browser-env.js"
    ]
  },
  "xo": {
    "space": 4,
    "parser": "babel-eslint",
    "esnext": true,
    "globals": [
      "window",
      "document",
      "self"
    ],
    "rules": {
      "padded-blocks": "off",
      "object-curly-spacing": [
        2,
        "always"
      ],
      "no-unused-expressions": "off",
      "import/prefer-default-export": "off",
      "no-duplicate-imports": "off"
    }
  }
}
