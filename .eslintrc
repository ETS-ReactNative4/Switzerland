{
    "parser": "babel-eslint",
    "extends": ["standard", "prettier", "prettier/react", "prettier/standard"],
    "plugins": ["react"],
    "rules": {
        "no-sequences": "off"
    },
    "globals": {
        "customElements": true,
        "CustomEvent": true,
        "fetch": true,
        "HTMLElement": true,
        "MutationObserver": true,
        "ResizeObserver": true,
        "ShadowRoot": true
    }
}
